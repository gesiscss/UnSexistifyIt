<body>
    <div ng-cloak>
        <div id="myModals">
            <!-- Continue Modal -->
            <!-- <div ng-if="localStorageNewUser && checkUrl() && round == 6 " ng-controller="playCtrl">
                <div class="modal fade" id="continuePlayingModal" role="dialog" data-focus="false">
                    <div class="modal-dialog modal-dialog-centered ">
                        <div class="modal-content">
